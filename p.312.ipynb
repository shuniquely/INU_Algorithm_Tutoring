   "source": [
    "S = input()\n",
    "result = int(S[0]) # 왼쪽 값부터 계산 시작\n",
    "\n",
    "for i in range(1, len(S)):\n",
    "    N = int(S[i])\n",
    "    if N <= 1 or result <= 1:# 왼쪽 값과 현재까지 계산된 값 둘 중 하나라도 0이거나 1이면 덧셈을 한다.\n",
    "        result += N\n",
    "        continue\n",
    "    result *= N # 그 외의 모든 경우는 곱셈을 한다.\n",
    "\n",
    "print(result)"
   ]
