    "array = list(S)\n",
    "\n",
    "array.sort()\n",
    "\n",
    "result = \"\"\n",
    "sum_Num = 0\n",
    "\n",
    "for i in array:\n",
    "    if 65 <= ord(i) <= 90:\n",
    "        result += i\n",
    "    else:\n",
    "        sum_Num += int(i)\n",
    "    \n",
    "result += str(sum_Num)\n",
    "\n",
    "print(result)"
